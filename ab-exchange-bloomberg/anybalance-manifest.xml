<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-exchange-bloomberg</id>
	<api flags="no_browser"/>
	<name>Bloomberg</name>
	<description html="true"><![CDATA[
	<p>Obtains information from http://www.bloomberg.com</p>
	]]></description>
	<instruction html="true"><![CDATA[
	<p>To use this provider you must know the symbols of indexes.</p>
	<p>You can find them on http://www.bloomberg.com/markets/symbolsearch, if you don`t know any</p>
	]]></instruction>
	<author>Stark &lt;support@starksoft.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Price" units="{@currency}"/>
		<counter id="trending" name="Trending difference"/>
		<counter id="trending_pcts" name="Trending pcts" units="%"/>
		<counter id="currency" name="Currency" type="text"/>
		<counter id="quote_show" name="Name" type="text"/>
		<counter id="open" name="Open"/>
		<counter id="volume" name="Volume"/>
		<counter id="prev_close" name="Previous Close"/>
		<counter id="days_range" name="Day`s Range" type="text"/>
		<counter id="52w_range" name="52wk Range" type="text"/>
	</counters>
	<keywords>rates, exchange, money, obligations, finance</keywords>
	<type>exchange</type>
	<country></country>
</provider>