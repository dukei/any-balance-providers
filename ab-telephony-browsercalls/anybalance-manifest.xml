<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-telephony-browsercalls</id>
	<api flags="no_browser|captcha"/>
	<name>BrowserCalls</name>
	<description html="true"><![CDATA[
	<p>Obtains information from https://www.browsercalls.com such as your credit, freedays.</p>
	]]></description>
	<author>Stark &lt;support@starksoft.ru&gt;</author>
