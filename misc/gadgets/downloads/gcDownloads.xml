<Module>
  <ModulePrefs title="__UP_projectName__ downloads" 
    title_url="http://code.google.com/p/__UP_projectName__/downloads"
    directory_title="Downloads by Google Code"
    description="View Google Code Project Hosting project downloads."
    screenshot="http://google-code-project-hosting-gadgets.googlecode.com/svn/trunk/downloads/downloads_screenshot.png"
    thumbnail="http://google-code-project-hosting-gadgets.googlecode.com/svn/trunk/downloads/downloads_thumbnail.png"
    author="Project Hosting Team"
    author_email="gadgets.feedback@gmail.com"
    author_location="San Francisco, CA"
    scrolling="no">
    <Require feature="setprefs" />
    <Require feature="dynamic-height" />
    <Require feature="settitle" />
  </ModulePrefs>
  <UserPref name="projectName" datatype="hidden" />
  <UserPref name="lastCount" datatype="hidden" />
  <Content type="html">
    <![CDATA[
    
<link href="https://google-code-project-hosting-gadgets.googlecode.com/svn/trunk/common/codegadgets.css" rel="stylesheet" type="text/css">    
<script type="text/javascript" src="https://jqueryjs.googlecode.com/files/jquery-1.2.6.pack.js"></script>    
<script type="text/javascript" src="https://google-code-project-hosting-gadgets.googlecode.com/svn/trunk/common/codegadgets.js"></script>
<!-- <script type="text/javascript" src="https://raw.github.com/douglascrockford/JSON-js/master/json2.js"></script> -->
<script type="text/javascript" src="https://any-balance-providers.googlecode.com/svn/misc/gadgets/downloads/gcDownloads.js?5"></script>

<div style="width:100%; height:100%; overflow: hidden;">
<div id="content_div">
</div>
</div>

    ]]>    
  </Content>

</Module>
