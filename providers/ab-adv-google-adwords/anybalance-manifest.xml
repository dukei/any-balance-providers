<?xml version="1.0" encoding="utf-8"?>
<provider>
    <id version="15">ab-adv-google-adwords</id>
	<api flags="no_browser" />
    <name>Google Adwords</name>
	<description html="true"><![CDATA[
	<p>Obtains information from Google Adwords personal account at <a href="https://adwords.google.com">https://adwords.google.com</a></p>
	<p>Stats are shown for a period selected in personal account</a>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="login/google"/>
		<module id="framework/gwt"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
    <counters>
        <counter id="balance" name="Balance" units=" {@currency}"/>
        <counter id="cost" name="Total Cost" units=" {@currency}"/>
        <counter id="clicks" name="Clicks" units=" clicks"/>
        <counter id="views" name="Views" units=" views"/>
        <counter id="rate" name="Rate" units="%"/>
        <counter id="average" name="Average Cost" units=" {@currency}"/>
        <counter id="currency" name="Currency" type="text"/>
        <counter id="last_pay_date" name="Last pay date" type="time"/>
        <counter id="last_pay_sum" name="Last pay sum" units=" {@currency}"/>
    </counters>
    <keywords>Google Adwords</keywords>
	<type>adv</type>
</provider>