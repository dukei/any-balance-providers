<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-bonus-etm</id>
	<api flags="no_browser"/>
	<name>ЭТМ</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о бонусах провайдера ЭТМ http://www.etm.ru/</p>
	<p>Информация берется из личного кабинета http://www.etm.ru/cat/discount-card.html </p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс"/>
		<counter id="bonus" name="Бонус" units=" Б"/>
		<counter id="cardnum" name="Номер карты" type="text"/>
		<counter id="level" name="Уровень" type="text"/>
		<counter id="discount" name="Скидка" type="text"/>
	</counters>
	<keywords></keywords>
	<type>bonus</type>
	<country>ru</country>
</provider>