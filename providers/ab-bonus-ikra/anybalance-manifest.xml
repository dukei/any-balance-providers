<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="5">ab-bonus-ikra</id>
	<api flags="no_browser"/>
	<name>Икра (Карусель)</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе в бонусной программе Икра сети супермаркетов Карусель</p>
	<p>Информация берется из личного кабинета https://prcab.x5club.ru/karusel/</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="crypt/sha1"/>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баллов на счёте"/>
		<counter id="bonuses" name="Баланс бонусов"/>
		<counter id="bonuses_avail" name="Доступно бонусов"/>
		<counter id="cardnum" name="Номер карты" type="text"/>
	</counters>
	<keywords></keywords>
	<type>bonus</type>
	<country>ru</country>
</provider>