<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
	<id version="2">ab-cell-att</id>
	<name>AT&amp;T (USA)</name>
	<description html="true"><![CDATA[
	<p>Obtains information from AT&amp;T personal account <a href=""></a></p>

	]]></description>
	<instruction html="true"><![CDATA[
	<p>If you are not registered for AT&amp;T you can register here: <a href="https://www.att.com/olam/slidRegistrationAction.olamexecute">https://www.att.com/olam/slidRegistrationAction.olamexecute</a></p>
	]]></instruction>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
    <depends>
  		<module id="library"/>
  	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Account balance" prefix="$ "/>
		<counter id="till" name="Use by" type="time" format="MMM dd"/>
		<counter id="expires" name="Expires" type="time" format="MMM dd"/>
		<counter id="planName" name="Plan name" type="text"/>
		<counter id="minPerMonth" name="Minutes per month" units=" min"/>
		<counter id="minAvailable" name="Minutes available" units=" min"/>
		<counter id="smsPerMonth" name="Messages per month" units=" sms"/>
		<counter id="smsAvailable" name="Messages available" units=" sms"/>
		<counter id="trafficPerMonth" name="Traffic per month" type="text"/>
		<counter id="trafficAvaliable" name="Traffic available" type="text"/>
		<counter id="status" name="Plan status" type="text"/>
	</counters>
	<keywords></keywords>
	<type>cell</type>
	<country>us</country>
</provider>