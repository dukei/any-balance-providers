<?xml version="1.0" encoding="utf-8"?>
<provider>
  <id version="56">ab-cell-intertelecom</id>
  <api flags="no_browser" />
  <name>Интертелеком</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе для пользователей мобильного оператора "Интертелеком", первого национального CDMA оператора Украины (http://www.intertelecom.ua).</p> 
	<p>Информация берется из cистемы самообслуживания (http://assa.intertelecom.ua).</p>
	]]></description>
	<instruction html="true"><![CDATA[
	<p>Настройки:</p>
	<ul>
	<li>Для получения пароля к системе самообслуживания необходимо зарегистрироваться на странице http://assa.intertelecom.ua/ru/reg</li> 
	<li>Для получения утерянного пароля зайдите в раздел восстановления пароля http://assa.intertelecom.ua/ru/recovery.</li>
	</ul>   
	]]></instruction>
  <author>DimaZUA &lt;DIMAZ.KHUA@gmail.com&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>main.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="ls" name="Лицевой счет" type="text"/>
    <counter id="loyalty" name="Лояльный стаж" type="time_interval" format="{@Y} г. {@mo} мес."/>
    <counter id="mobsubscr" name="Абонентский стаж" type="time_interval" format="{@Y} г. {@mo} мес."/>
    <counter id="phonet" name="Номер телефона" type="text"/>
    <counter id="mobphonet" name="Номер телефона мобильный" type="text"/>
    <counter id="contphone" name="Номер телефона контактный" type="text"/>

    <counter id="min1" prefix="{@pref_min1}" name="Минуты 1" type="time_interval" format="{@H} ч. {@min} мин."/>
    <counter id="min2" prefix="{@pref_min2}" name="Минуты 2" type="time_interval" format="{@H} ч. {@min} мин."/>
    <counter id="min3" prefix="{@pref_min3}" name="Минуты 3" type="time_interval" format="{@H} ч. {@min} мин."/>
    <counter id="min4" prefix="{@pref_min4}" name="Минуты 4" type="time_interval" format="{@H} ч. {@min} мин."/>
    <counter id="min5" prefix="{@pref_min5}" name="Минуты 5" type="time_interval" format="{@H} ч. {@min} мин."/>

    <counter id="date_min1" prefix="{@pref_min1}" name="Срок действия минут 1" type="time" format="dd.MM.yyyy"/>
    <counter id="date_min2" prefix="{@pref_min2}" name="Срок действия минут 2" type="time" format="dd.MM.yyyy"/>
    <counter id="date_min3" prefix="{@pref_min3}" name="Срок действия минут 3" type="time" format="dd.MM.yyyy"/>
    <counter id="date_min4" prefix="{@pref_min4}" name="Срок действия минут 4" type="time" format="dd.MM.yyyy"/>
    <counter id="date_min5" prefix="{@pref_min5}" name="Срок действия минут 5" type="time" format="dd.MM.yyyy"/>

    <counter id="traffic1" prefix="{@pref_traffic1}" name="Трафик 1" units=" mb"/>
    <counter id="traffic2" prefix="{@pref_traffic2}" name="Трафик 2" units=" mb"/>
    <counter id="traffic3" prefix="{@pref_traffic3}" name="Трафик 3" units=" mb"/>
    <counter id="traffic4" prefix="{@pref_traffic4}" name="Трафик 4" units=" mb"/>
    <counter id="traffic5" prefix="{@pref_traffic5}" name="Трафик 5" units=" mb"/>

    <counter id="date_traffic1" prefix="{@pref_traffic1}" name="Срок действия трафика 1" type="time" format="dd.MM.yyyy"/>
    <counter id="date_traffic2" prefix="{@pref_traffic2}" name="Срок действия трафика 2" type="time" format="dd.MM.yyyy"/>
    <counter id="date_traffic3" prefix="{@pref_traffic3}" name="Срок действия трафика 3" type="time" format="dd.MM.yyyy"/>
    <counter id="date_traffic4" prefix="{@pref_traffic4}" name="Срок действия трафика 4" type="time" format="dd.MM.yyyy"/>
    <counter id="date_traffic5" prefix="{@pref_traffic5}" name="Срок действия трафика 5" type="time" format="dd.MM.yyyy"/>

  </counters>
  <keywords>
    мобильный национальный оператор cdma украина
  </keywords>
  <type>
    cell
  </type>
  <country>
    ua
  </country>
</provider>
