<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="22">ab-cell-tele2-kz</id>
	<api flags="no_browser|captcha"/>
	<name>Теле2 Казахстан</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию из личного кабинета Теле2 Казахстан (http://www.tele2.kz/)</p>
	]]></description>
	<instruction html="true"><![CDATA[
	<p>Чтобы получить пароль перейдите по ссылке https://tele2.kz/me/sign и введите свой номер телефона</p>
	]]></instruction>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<author>DimaZUA &lt;DIMAZ.KHUA@gmail.com&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" 〒"/>
		<counter id="price" name="Стоимость" units=" 〒{@periodicityname}"/>
		<counter id="nextWriteOff" name="Тариф дейтвует до" type="time" format="dd.MM.yyyy"/>
		<counter id="internet_trafic" name="Интернет-пакеты" units="{@internet_trafic_left_units} Мб"/>
		<counter id="internet_trafic_night" name="Интернет-пакеты (ночь)" units="{@internet_trafic_night_left_units} Мб"/>
		<counter id="sms_left" name="Остаток SMS" units="{@sms_left_units} sms"/>
		<counter id="mms_left" name="Остаток MMS" units="{@mms_left_units} mms"/>
		<counter id="min_left" name="Остаток минут" units="{@min_left_units} мин"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="phone" name="Телефон" type="text"/>
	</counters>
	<keywords>теле2 Казахстан</keywords>
	<type>cell</type>
	<country>kz</country>
</provider>
