<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
	<id version="4">ab-cell-whyfly</id>
	<name>WhyFly</name>
	<description html="true">
		<![CDATA[
	<p>Предоставляет информацию о балансе и выбранном лицевом счете для пользователей оператора связи WhyFly https://whyfly.ru/.</p>
	<p>Информация берется из личного кабинета https://lk.whyfly.ru/</p>
	]]>
	</description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="month_fee" name="Абонентская плата, в мес." units=" р"/>
		<counter id="sms" name="MMS и SMS (остаток)" units=" шт"/>
		<counter id="calls" name="Местные вызовы (остаток)" units=" мин"/>
		<counter id="internet" name="Мобильный интернет (остаток)" units=" Mb"/>
		<counter id="status" name="Cтатус" type="text"/>
	</counters>
	<keywords>интернет</keywords>
	<type>cell</type>
	<country>ru</country>
</provider>