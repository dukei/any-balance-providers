<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-exchange-alfabank</id>
	<api flags="no_browser"/>
	<name>Альфа-Банк курс валют</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о курсе валют Альфа-Банка <a href="https://alfabank.ru/">https://alfabank.ru/</a></p>
	<p>Информация берется с RSS страницы <a href="https://alfabank.ru/_/rss/_currency.html">https://alfabank.ru/_/rss/_currency.html</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<!-- <preferences>preferences.xml</preferences> -->
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="buyUSD" name="USD покупка" units=" р"/>
    <counter id="sellUSD" name="USD продажа" units=" р"/>
    <counter id="buyEUR" name="EUR покупка" units=" р"/>
    <counter id="sellEUR" name="EUR продажа" units=" р"/>
		<counter id="buyGBP" name="GBP покупка" units=" р"/>
    <counter id="sellGBP" name="GBP продажа" units=" р"/>
    <counter id="buyCHF" name="CHF покупка" units=" р"/>
    <counter id="sellCHF" name="CHF продажа" units=" р"/>
    <counter id="date" name="Дата" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords>обмен валют</keywords>
	<type>exchange</type>
	<country>ru</country>
</provider>
