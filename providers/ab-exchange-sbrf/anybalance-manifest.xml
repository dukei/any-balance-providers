<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
  <id major_version="1.0" version="12">ab-exchange-sbrf</id>
  <name>Курсы валют СБ РФ</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о курсах валют от банка "Сбербанк" со страницы https://www.sberbank.ru/ru/quotes/currencies .</p>
	]]></description>
  <author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
  <depends>
    <module id="library"/>
    <module id="framework/sbrf"/>
  </depends>
  <files>
	  <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <history>history.xml</history>
    <js>main.js</js>
  </files>
  <counters>
    <counter id="usd_purch" name="USD покупка" units=" р"/>
    <counter id="usd_sell" name="USD продажа" units=" р"/>
    <counter id="eur_purch" name="EUR покупка" units=" р"/>
    <counter id="eur_sell" name="EUR продажа" units=" р"/>
    <counter id="date" name="Дата курса" type="time" format="dd/MM/yyyy"/>
  </counters>
  <keywords>Курсы, валюта, доллар, евро, фунт, сбербанк</keywords>
	<type>exchange</type>
	<country>ru</country>
</provider>