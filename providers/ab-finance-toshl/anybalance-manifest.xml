<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-finance-toshl</id>
	<api flags="no_browser"/>
	<name>TOSHL Finance</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Toshl <a href="https://toshl.com/">https://toshl.com/</a></p>
	<p>Информация берется из личного кабинета <a href="https://toshl.com/timeline/">https://toshl.com/timeline/</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Осталось потратить" units=" {@currency}"/>
		<counter id="expenses" name="Расходы" units=" {@currency}"/>
		<counter id="incomes" name="Доходы" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
	</counters>
	<keywords></keywords>
	<type>finance</type>
	<country>ru</country>
</provider>