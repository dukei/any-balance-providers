<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser" />
	<id major_version="2.0" version="12">ab-hosting-jino</id>
	<name>Джино</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе, остатке дискового и почтового пространства хостинг-провайдера Джино http://jino.ru/</p>
	<p>Информация берется из контрольной панели https://cp-hosting.jino.ru/</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="deadline" name="Дата отключения" type="time" format="dd/MM/yyyy"/>
		<counter id="monthly_fee" name="Оплата в месяц" units=" р"/>
		<counter id="daily_fee" name="Оплата в день" units=" р"/>
		<counter id="storage_used" name="Дисковое простарнство - использовано Mb" units=" Mb"/>
		<counter id="storage_total" name="Дисковое простарнство - всего Mb" units=" Mb"/>
		<counter id="storage_left" name="Дисковое простарнство - осталось Mb" units=" Mb"/>
		<counter id="storage_percent" name="Дисковое простарнство - использовано %" units=" %" type="text" />
		<counter id="storage_percent_left" name="Дисковое простарнство - осталось %" units=" %" type="text" />
		<counter id="mail_used" name="Почта - использовано Mb" units=" Mb"/>
		<counter id="mail_total" name="Почта - всего Mb" units=" Mb"/>
		<counter id="mail_left" name="Почта - осталось Mb" units=" Mb"/>
		<counter id="mail_percent" name="Почта - использовано %" units=" %" type="text" />
		<counter id="mail_percent_left" name="Почта - осталось %" units=" %" type="text" />
	</counters>
	<keywords></keywords>
	<type>hosting</type>
	<country>ru</country>
</provider>