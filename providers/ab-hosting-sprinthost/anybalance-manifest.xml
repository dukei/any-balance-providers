<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="4">ab-hosting-sprinthost</id>
	<api flags="no_browser"/>
	<name>Sprinthost (хостинг сайтов)</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Sprinthost http://sprinthost.ru/</p>
	<p>Информация берется из личного кабинета https://cp.sprinthost.ru/</p>
	]]></description>
	<depends>
		<module id="library"/>
	</depends>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" p"/>
		<counter id="bonus" name="Бонусы" units=" р"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="accountID" name="Лицевой счёт" type="text"/>
		<counter id="currentTariff" name="Текущий тариф" type="text"/>
		<counter id="tariffCost" name="Стоимость тарифа" units=" р"/>
		<counter id="dBonus" name="Бонусы на домены" />
		<counter id="daysLeft" name="Осталось дней"/>
		<counter id="diskSpaceUsed" name="Использовано места на диске" units=" Мб"/>
		<counter id="domains" name="Количество доменов"/>
		<counter id="vdomains" name="Количество сайтов"/>
		<counter id="FTPs" name="Количество входов FTP"/>
		<counter id="dataBases" name="Количество баз данных"/>
	</counters>
	<keywords></keywords>
	<type>hosting</type>
	<country>ru</country>
</provider>