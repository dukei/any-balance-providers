<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
	<id version="22">ab-internet-beeline</id>
	<name>Билайн интернет</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе, бонусах и тарифном плане для пользователей Билайн интернет.</p>       
	<p>Информация берется из личного кабинета http://www.beeline.ru/login либо из https://cabinet.beeline.ru</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="parsers/jsonpath"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="balance_tv" name="Баланс ТВ" units=" р"/>
		<counter id="balance_phone" name="Баланс Телефон" units=" р"/>
		<counter id="bonus" name="Бонусы" units=" р"/>
		<counter id="traffic" name="Включенный трафик" units=" Гб"/>
		<counter id="topay" name="Сумма к оплате" units=" р"/>
		<counter id="abon" name="Ежемесячный платеж" units=" р"/>
		<counter id="till" name="Конец расчетного периода" type="time"/>
		<counter id="address" name="Адрес подключения" type="text"/>
		<counter id="bill" name="Счет" type="text"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="status_internet" name="Интернет (статус)" type="text"/>
		<counter id="status_tv" name="Телевидение (статус)" type="text"/>
	</counters>
	<keywords>интернет, билайн, beeline</keywords>
	<type>internet</type>
	<country>ru</country>
</provider>
