<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="3">ab-internet-campus-networks</id>
	<api flags="no_browser"/>
	<name>Campus Networks</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Campus Networks <a href="http://www.campus.rv.ua/">http://www.campus.rv.ua/</a></p>
	<p>Информация берется из личного кабинета <a href="https://stat.campus-rv.net/">https://stat.campus-rv.net/</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" грн"/>
		<counter id="credit" name="Кредит" units=" грн"/>
		<counter id="cost_tariff" name="Стоимость тарифа" units=" грн"/>
<!--		<counter id="cost_IP" name="Абонплата за реальный IP" units=" грн"/> -->
		<counter id="days_left" name="Интернет выключится через" units=" дн"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="UID" name="UID" type="text"/>
		<counter id="agreement_num" name="Номер договора" type="text"/>
		<counter id="user_status" name="Статус пользователя" type="text"/>
		<counter id="turbo_status" name="Статус услуги Турбо" type="text"/>
<!--		<counter id="internet_status" name="Статус интернета" type="text"/> -->
	</counters>
	<keywords></keywords>
	<type>internet</type>
	<country>ua</country>
</provider>