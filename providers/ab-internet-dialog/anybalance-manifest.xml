<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="5">ab-internet-dialog</id>
	<api flags="no_browser"/>
	<name>Диалог г. Калининград</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию  из личного кабинета интернет провайдера Диалог <a href="http://tis-dialog.ru/">http://tis-dialog.ru/</a> г. Калининград</p>
	]]></description>
	<author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
	<depends>
	<module id="library"/>
</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="trafficExt" name="Внешний трафик" units=" Gb"/>
		<counter id="trafficCity" name="Городской трафик" units=" Gb"/>
		<counter id="status" name="Состояние" type="text"/>
		<counter id="licschet" name="Лицевой счет" type="text"/>
		<counter id="userName" name="ФИО" type="text"/>
		<counter id="trafficExtIn" name="Внешний трафик (вх)" units=" Gb"/>
		<counter id="trafficExtOut" name="Внешний трафик (исх)" units=" Gb"/>
		<counter id="trafficCityIn" name="Городской трафик (вх)" units=" Gb"/>
		<counter id="trafficCityOut" name="Городской трафик (исх)" units=" Gb"/>
		<counter id="speed" name="Скорость подключения" type="text"/>
		<counter id="online" name="Дата и время подключения" type="text"/>
		<counter id="ip" name="IP-адрес" type="text"/>
		<counter id="user_address" name="Адрес подключения" type="text"/>
	</counters>
	<keywords>интернет, калининград, диалог</keywords>
	<type>internet</type>
	<country>ru</country>
</provider>
