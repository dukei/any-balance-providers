<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id major_version="3.0" version="29">ab-internet-kievstar</id>
	<api flags="no_browser"/>
	<name>Киевстар (домашний интернет)</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе и бонусных баллах для пользователей интернет-провайдера "Киевстар".</p>
	<p>Информация берется из личного кабинета (http://my.kyivstar.ua).</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<author>Viacheslav Sychov &lt;viacheslav.sychov@gmail.com&gt;</author>
	<depends>
		<module id="library"/>
		<module id="framework/gwt"/>
		<module id="login/kyivstar"/>
		<module id="parsers/jsonpath"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="bonusValue" name="Бонусный баланс" units=" б"/>
		<counter id="bonusDate" name="Срок действия бонусов" type="text"/>
		<counter id="licschet" name="Лицевой счёт" type="text"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="name" name="Имя" type="text"/>
		<counter id="phone" name="Номер телефона" type="text"/>
		<counter id="date_start" name="Дата подключения" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords>Домашний интернет, интернет, kyivstar, киевстар, украина</keywords>
	<type>internet</type>
	<country>ua</country>
</provider>
