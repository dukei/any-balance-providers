<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-internet-metroset</id>
	<api flags="no_browser"/>
	<name>Метросеть</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Метросеть http://www.metro-set.ru/</p>
	<p>Информация берется из личного кабинета https://cabinet.metro-set.ru/index.php</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id ="crypt/md5"/>
		<module id ="crypt/base64"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="bonus" name="Бонус" units=" д"/>
		<counter id="internetStatus" name="Статус интернета" type="text"/>
		<counter id="metrosetStatus" name="Метросеть статус" type="text"/>
		<counter id="agreement" name="Номер договора" type="text"/>
		<counter id="deadline" name="Активен до" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords></keywords>
	<type>internet</type>
	<country>ru</country>
</provider>