<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-internet-mosinter</id>
	<api flags="no_browser"/>
	<name>МосИнтер</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера МосИнтер <a href="http://mosinter.net/">http://mosinter.net/</a></p>
	<p>Информация берется из личного кабинета</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="traffic" name="Трафик" units=" Mb"/>
		<counter id="status" name="Доступ в интернет" type="text"/>
		<counter id="agreementID" name="Номер договора" type="text"/>
		<counter id="startDate" name="Дата начала периода" type="time" format="dd/MM/yyyy"/>
		<counter id="deadline" name="Услуги оплачены до" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords></keywords>
	<type>internet</type>
	<country>ru</country>
</provider>