    <counter id="trafficAbon" name="Включенный в АП трафик" units="Gb"/>
    <counter id="trafficExtra" name="Трафик сверх АП" units="Gb"/>
    <counter id="period_end" name="Конец расчетного периода" type="time" format="dd/MM/yyyy"/>
    <counter id="fio" name="ФИО" type="text"/>
  </counters>
  <keywords>
    интернет
  </keywords>
