<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-internet-wetel</id>
	<api flags="no_browser"/>
	<name>Wetel</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе интернет-провайдера Wetel http://www.wetel.ru/</p>
	<p>Информация берется из личного кабинета http://www.wetel.ru/user</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="accountNumber" name="Номер счёта" type="text"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="currentTariff" name="Текущий тариф" type="text"/>
		<counter id="deadline" name="Конец учётного периода" type="time" format="dd/MM/yyyy"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="phone" name="Телефон" type ="text"/>
		<counter id="adress" name="Адрес" type="text"/>
		<counter id="promisedPayment" name="Отложенный платёж" type="text"/>
	</counters>
	<keywords>ветел</keywords>
	<type>internet</type>
	<country>ru</country>
</provider>