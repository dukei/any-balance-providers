<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-money-baitushum</id>
	<api flags="no_browser"/>
	<name>Банк Бай-Түшүм</name>
    <description html="true"><![CDATA[
    <p>Получает текущий остаток и другие параметры продуктов Банкa Бай-Түшүм http://www.baitushum.kg/kg/, используя интернет-банк (https://online.baitushum.kg/WWWCOR/).</p>
    ]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="nadapter"/>
		<module id="extract/abe-money-baitushum"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Остаток" units=" {@currency}"/>
		<counter id="available" name="Доступно" units=" {@currency}"/>
		<counter id="till" name="Срок действия карты" type="time" format="dd/MM/yyyy"/>
        <counter id="date_start" name="Дата открытия" type="time" format="dd/MM/yyyy"/>
        <counter id="last_movement" name="Последнее движение" type="time" format="dd/MM/yyyy"/>
        <counter id="fio" name="ФИО" type="text"/>
        <counter id="currency" name="Валюта" type="text"/>
        <counter id="cardnum" name="Номер карты" type="text"/>
        <counter id="type" name="Тип" type="text"/>
        <counter id="status" name="Статус" type="text"/>
        <counter id="accnum" name="Номер счёта" type="text"/>
	</counters>
	<keywords>тушум</keywords>
	<type>money, bank</type>
	<country>kg</country>
</provider>