<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="4">ab-money-beeline</id>
	<api flags="no_browser"/>
	<name>Платежный кабинет Билайн</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе на карте Билайн http://bank.beeline.ru</p>
	<p>Информация берется из платежного кабинета https://bank.beeline.ru/</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="extra" name="Экстра-бонусы" units=" Б"/>
		<counter id="last_op_sum" name="Сумма последней операции" units=" р"/>
		<counter id="last_op_date" name="Дата последней операции" type="time" format="dd/MM/yyyy"/>
		<counter id="last_op_descr" name="Описание последней операции" type="text"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="currency" name="Валюта" type="text"/>
	</counters>
	<keywords></keywords>
	<type>money, wallet</type>
	<country>ru</country>
</provider>