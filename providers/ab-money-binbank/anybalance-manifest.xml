<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="13">ab-money-binbank</id>
	<api flags="no_browser"/>
	<name>Бинбанк</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию по картам Бин Банка Москва.</p>
	<p>Информация берется из интернет-банка https://i.binbank.ru</p>
	]]></description>
	<instruction html="true"><![CDATA[
	<p>Для подключения к Интернет-банку зарегистрируйтесь здесь: https://i.binbank.ru/register</p>
	]]></instruction>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="nadapter"/>
		<module id="extract/abe-money-binbank"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="airmiles" name="Миль" units=" М"/>
		<counter id="blocked" name="Заблокировано" units=" {@currency}"/>
		<counter id="cardnum" name="Номер карты" type="text"/>
		<counter id="accnum" name="Номер счета" type="text"/>
		<counter id="credit_used" name="Использованный кредит" units=" {@currency}"/>
		<counter id="credit_pay_to" name="Оплатить до" type="time" format="dd/MM/yyyy"/>
		<counter id="credit_pay_sum" name="Сумма платежа по кредиту" units=" {@currency}"/>
		<counter id="till" name="Действует до" type="time" format="MM/yy"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="type" name="Тип" type="text"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="currency" name="Валюта" type="text"/>
	</counters>
	<keywords></keywords>
	<type>money, bank</type>
	<country>ru</country>
</provider>