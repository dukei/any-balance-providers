<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-money-credit-agricole</id>
	<api flags="no_browser"/>
	<name>Credit Agricole</name>
	<description html="true"><![CDATA[
	<p>Получает текущий остаток и другие параметры карт и депозитов Банка Credit Agricole, используя интернет-банк https://online.credit-agricole.ua/</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="nadapter"/>
		<module id="extract/abe-money-credit-agricole"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Остаток" units=" {@currency}"/>
		<counter id="pct" name="Процентная ставка" units="%"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="type" name="Тип" type="text"/>
		<counter id="agreement" name="Номер договора" type="text"/>
		<counter id="accnum" name="Номер счета" type="text"/>
		<counter id="holder" name="Держатель карты" type="text"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="date_start" name="Дата открытия" type="time" format="dd/MM/yyyy"/>
		<counter id="till" name="Срок действия" type="time" format="dd/MM/yyyy"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="phone" name="Телефон" type="text"/>
	</counters>
	<keywords>агриколь</keywords>
	<type>money, bank</type>
	<country>ua</country>
</provider>