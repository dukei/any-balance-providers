<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-money-economix</id>
	<api flags="no_browser"/>
	<name>Экономикс-Банк</name>
	<description html="true"><![CDATA[
	<p>Получает текущий остаток и другие параметры счетов Экономикс-Банка используя систему веб-банкинга https://ibank.economiks.ru:2443/web_banking/ </p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="nadapter"/>
		<module id="extract/abe-money-economix"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Остаток" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="cardnum" name="Номер карты/кредита/депозита" type="text"/>
		<counter id="accnum" name="Номер счета" type="text"/>
		<counter id="name" name="Имя" type="text"/>
		<counter id="name_patronymic" name="Отчество" type="text"/>
		<counter id="name_last" name="Фамилия" type="text"/>
		<counter id="agreement" name="Номер договора" type="text"/>
		<counter id="date_start" name="Дата открытия" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords>economiks</keywords>
	<type>money, bank</type>
	<country>ru</country>
</provider>