<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-money-moneta</id>
	<api flags="no_browser"/>
	<name>Moneta.ru</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе USD, RUB и EUR счетов в платежной системе Монета</p>
	<p>Информация берется из личного кабинета https://moneta.ru/desktop.htm</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="rub_balance" name="Баланс RUB" units=" р"/>
		<counter id="rub_acc" name="Счет RUB" type="text"/>
		<counter id="usd_balance" name="Баланс USD" units=" $"/>
		<counter id="usd_acc" name="Счет USD" type="text"/>
		<counter id="eur_balance" name="Баланс EUR" units=" €"/>
		<counter id="eur_acc" name="Счет EUR" type="text"/>
		<counter id="type" name="Тип счета" type="text"/>
	</counters>
	<keywords></keywords>
	<type>money, wallet</type>
	<country>ru</country>
</provider>