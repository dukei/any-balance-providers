<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="15">ab-money-mybank</id>
	<api flags="no_browser"/>
	<name>МТБанк (Беларусь)</name>
	<description html="true"><![CDATA[
	<p>Получает текущий остаток и другие параметры карт МТБанка <a href="http://www.mtbank.by/">http://www.mtbank.by/</a></p>
	<p>Информация берется из личного кабинета <a href="https://mybank.by/">https://mybank.by/</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="own" name="Собственные средства" units=" {@currency}"/>
		<counter id="credit" name="Доступная рассрочка" units=" {@currency}"/>
		<counter id="limit" name="Лимит" units=" {@currency}"/>
		<counter id="debt" name="Задолженность" units=" {@currency}"/>
		<counter id="nachisl" name="Начисл. проценты" units=" {@currency}"/>
		<counter id="grace_pay" name="Минимальный платеж" units=" {@currency}"/>
		<counter id="grace_pay_till" name="Дата минимального платежа" type="time" format="dd/MM/yyyy"/>
		<counter id="cardname" name="Наименование карты" type="text"/>
		<counter id="cardnum" name="Номер карты" type="text"/>
		<counter id="order_num" name="Номер договора" type="text"/>
		<counter id="halava_bonus" name="Бонус карты Халва Плюс" units=" бал"/>
	</counters>
	<keywords></keywords>
	<type>money, bank</type>
	<country>by</country>
</provider>