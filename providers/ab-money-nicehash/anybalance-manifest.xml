<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="6">ab-money-nicehash</id>
	<api flags="no_browser"/>
	<name>NiceHash</name>
	<description html="true"><![CDATA[
	<p>Provides nicehash.com unpaid balances for a given BTC wallet.</p>
	]]></description>
	<author>Borislip &lt;borislip@gmail.com&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>btcutil.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="Total" name="Total" units="{@btcunits}"/>
		<counter id="Scrypt" name="Scrypt" units="{@btcunits}"/>
		<counter id="SHA256" name="SHA256" units="{@btcunits}"/>
		<counter id="ScryptANf" name="Scrypt-A.-Nf." units="{@btcunits}"/>
		<counter id="X11" name="X11" units="{@btcunits}"/>
		<counter id="X13" name="X13" units="{@btcunits}"/>
		<counter id="Keccak" name="Keccak" units="{@btcunits}"/>
		<counter id="X15" name="X15" units="{@btcunits}"/>
		<counter id="Nist5" name="Nist5" units="{@btcunits}"/>
		<counter id="NeoScrypt" name="NeoScrypt" units="{@btcunits}"/>
		<counter id="Lyra2RE" name="Lyra2RE" units="{@btcunits}"/>
		<counter id="WhirlpoolX" name="WhirlpoolX" units="{@btcunits}"/>
		<counter id="Qubit" name="Qubit" units="{@btcunits}"/>
		<counter id="Quark" name="Quark" units="{@btcunits}"/>
		<counter id="Axiom" name="Axiom" units="{@btcunits}"/>
		<counter id="Lyra2REv2" name="Lyra2REv2" units="{@btcunits}"/>
		<counter id="ScryptJaneNf16" name="ScryptJaneNf16" units="{@btcunits}"/>
		<counter id="Blake256r8" name="Blake256r8" units="{@btcunits}"/>
		<counter id="Blake256r14" name="Blake256r14" units="{@btcunits}"/>
		<counter id="Blake256r8vnl" name="Blake256r8vnl" units="{@btcunits}"/>
