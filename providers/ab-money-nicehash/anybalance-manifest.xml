		<counter id="Decred" name="Decred" units="{@btcunits}"/>
		<counter id="CryptoNight" name="CryptoNight" units="{@btcunits}"/>
		<counter id="Lbry" name="Lbry" units="{@btcunits}"/>
		<counter id="Equihash" name="Equihash" units="{@btcunits}"/>
		<counter id="btcunits" name="Units" type="text"/>
	</counters>
	<keywords>bitcoin,BTC,NiceHash,pool,mining,биткоин</keywords>
	<type>money, wallet</type>
	<country></country>
