<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id major_version="2.8" version="27">ab-money-sberbank-ua</id>
	<api flags="no_browser"/>
	<name>Ощадбанк ОнЛ@йн (Україна)</name>
	<description html="true"><![CDATA[
	<p>Отримує поточний залишок та інші параметри карт Ощадбанку в України, використовуючи систему Ощадбанк-онлайн https://online.oschadnybank.com/wb </p>
	]]></description>
	<instruction html="true"><![CDATA[
	<p>Якщо у вас немає доступу до Ощадбанк-онлайн: перейдіть на сторінку https://online.oschadnybank.com/wb/#enrollment</p>
	]]></instruction>
	<depends>
		<module id="library"/>
		<module id="nadapter"/>
		<module id="extract/abe-money-sberbank-ua"/>
	</depends>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Залишок" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="till" name="Дата закінчення дії" type="time" format="MM/yyyy"/>
		<counter id="maxlimit" name="Кредитний ліміт" units=" {@currency}"/>
		<counter id="debt" name="Загальна заборгованість" units=" {@currency}"/>
		<counter id="mz" name="Мобільні заощадження" units=" {@currency}"/>
		<counter id="rr" name="Розрахунковий рахунок" type="text"/>
		<counter id="cardNumber" name="Номер карти" type="text"/>
		<counter id="fio" name="ПIБ" type="text"/>
		<counter id="mess" name="Повідомлення"/>
	</counters>
	<keywords>сбербанк онлайн, sberbank online, сберегательный банк, ощадбанк, ощадний банк</keywords>
	<type>money, bank</type>
	<country>ua</country>
</provider>