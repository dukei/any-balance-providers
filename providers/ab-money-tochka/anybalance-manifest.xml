<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-money-tochka</id>
	<api flags="no_browser"/>
	<name>Точка</name>
	<description html="true"><![CDATA[
	<p>Получает текущий остаток и другие параметры карт и счетов банка Точка, используя интернет-банкинг https://i.tochka.com/ </p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="nadapter"/>
		<module id="extract/abe-money-tochka"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Остаток" units=" {@currency}"/>
		<counter id="credit" name="Кредит" units=" {@currency}"/>
		<counter id="available" name="Доступно" units=" {@currency}"/>
		<counter id="reserved" name="Зарезервировано" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="accnum" name="Номер счета" type="text"/>
		<counter id="type" name="Тип счета/карты" type="text"/>
		<counter id="name" name="Пользователь" type="text"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="cardholder" name="Владелец" type="text"/>
		<counter id="till" name="Срок действия" type="time" format="dd/MM/yyyy"/>
		<counter id="date_start" name="Начало действия" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords>точка</keywords>
	<type>money, bank</type>
	<country>ru</country>
</provider>