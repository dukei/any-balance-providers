<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
  <id version="5">ab-money-uniastrum</id>
  <name>Юниаструм банк</name>   
  <description>
    Получает текущий остаток и другие параметры карт/счетов/депозитов/кредитов банка Юниаструм (http://uniastrum.ru/), используя интернет-банк https://1bank.uniastrum.ru
  </description>
  <author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>library.js</js>
    <js>main.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="balance" name="Остаток" units=" {@currency}"/>
    <counter id="limit" name="Лимит кредита" units=" {@currency}"/>
    <counter id="debt" name="Задолженность"/>
	<counter id="minpay" name="К оплате"/>
    <counter id="minpaytill" name="Срок оплаты" type="time" format="dd/MM/yyyy"/>
    <counter id="type" name="Тип" type="text"/>
    <counter id="pct" name="Процентная ставка" units="%"/>
    <counter id="pcts" name="Сумма начисленных процентов"/>
    <counter id="till" name="Срок действия" type="time" format="dd/MM/yyyy"/>
    <counter id="status" name="Статус" type="text"/>
    <counter id="cardnum" name="Номер карты" type="text"/>
    <counter id="accnum" name="Номер счета" type="text"/>
    <counter id="fio" name="Владелец" type="text"/>
    <counter id="currency" name="Валюта" type="text"/>
	<counter id="all" name="Сводка" type="text"/>
  </counters>
  <keywords>
    интернет-банкинг
  </keywords>
  <type>
    money
  </type>
  <country>
    ru
  </country>
</provider>