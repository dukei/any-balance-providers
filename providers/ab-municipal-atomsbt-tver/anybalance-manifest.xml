<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="3">ab-municipal-atomsbt-tver</id>
	<api flags="no_browser"/>
	<name>АТОМЭНЕРГОСБЫТ (Тверь)</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера АТОМЭНЕРГОСБЫТ (Тверь) <a href="http://atomsbt.ru/">http://atomsbt.ru/</a></p>
	<p>Информация берется из личного кабинета <a href="http://atomsbt.ru/klientam/kabinet_tver/">http://atomsbt.ru/klientam/kabinet_tver/</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
    <depends>
        <module id="library"/>
    </depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="accrued" name="Начислено в текущем месяце" units=" p"/>
		<counter id="paid" name="Оплачено в текущем месяце" units=" p"/>
        <counter id="saldo" name="Исходящее сальдо" units=" p"/>
        <counter id="in_saldo" name="Входящее сальдо" units=" p"/>
        <counter id="adress" name="Адрес" type="text"/>
        <counter id="period" name="Текущий период" type="time" format="MM/yyyy"/>
		<counter id="device_value" name="Показание счётчика"/>
	</counters>
	<keywords></keywords>
	<type>municipal</type>
	<country>ru</country>
</provider>