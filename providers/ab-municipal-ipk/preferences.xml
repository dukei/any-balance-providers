<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
    <EditTextPreference
        title="E-mail адрес"
        inputType="textEmailAddress"
        key="email"
        summary="||{@s}"
        obligatory="true"
        dialogTitle="Номер договора"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        dialogMessage="Введите e-mail адрес, используемый для входа на сайт">
    </EditTextPreference>
    <EditTextPreference
        title="Пароль"
        key="password"
        summary="||***********"
        obligatory="true"
        dialogTitle="Пароль"
        positiveButtonText="ОК"
        negativeButtonText="Отмена"
        dialogMessage="Введите пароль, используемый для входа на сайт"
        inputType="textPassword">
    </EditTextPreference>
</PreferenceScreen>
