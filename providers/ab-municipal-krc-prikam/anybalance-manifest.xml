		<counter id="adress" name="Адрес" type="text"/>
		<counter id="area" name="Площадь" units="кв.м."/>
		<counter id="people" name="Количество проживающих" type="text"/>
		<counter id="provider" name="Поставщик услуг" type="text"/>
