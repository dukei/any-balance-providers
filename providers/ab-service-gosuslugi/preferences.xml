<PreferenceScreen>
	<EditTextPreference
		title="СНИЛС/Телефон/Email"
		key="login"
		summary="||{@s}"
		obligatory="true"
		dialogTitle="СНИЛС/Телефон/Email"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите СНИЛС (без пробелов и разделителей, 11 цифр подряд), телефон (10 цифр подряд) или введите Email.">
	</EditTextPreference>
	<EditTextPreference
		title="Пароль"
		key="password"
		summary="||***********"
		obligatory="true"
		dialogTitle="Пароль"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите пароль, используемый для входа в Госуслуги."
		inputType="textPassword">
	</EditTextPreference>
