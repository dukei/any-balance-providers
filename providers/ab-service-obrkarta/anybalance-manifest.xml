<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-service-obrkarta</id>
	<api flags="no_browser"/>
	<name>Образовательная карта</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера "Образовательная карта" <a href="http://obrkarta.ru/">http://obrkarta.ru/</a></p>
	<p>Информация берется из личного кабинета <a href="http://obrkarta.ru/auth/">http://obrkarta.ru/auth/</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
