		<counter id="without_card" name="Текущее количество покупок без предъявления карты"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="status" name="Состояние договора" type="text"/>
		<counter id="acc" name="Номер лицевого счета" type="text"/>
	</counters>
	<keywords></keywords>
	<type>service</type>
	<country>ru</country>
