<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
	<id version="6">ab-telephony-comtube</id>
	<name>ComTube</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о текущем балансе, балансе инвестиционного счета, валюте счета и номере внутри системы для пользователей провайдера IP-телефонии ComTube (http://www.comtube.com).</p>   
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="invest" name="Баланс инвестиционного счета" units=" {@currency}"/>
		<counter id="partner_balance" name="Партнерский счет" units=" {@currency}"/>
		<counter id="partner_available" name="Доступно для вывода" units=" {@currency}"/>
		<counter id="number" name="Номер ComTube" type="text"/>
		<counter id="licschet" name="Лицевой счет" type="text"/>
		<counter id="currency" name="Валюта" type="text"/>
	</counters>
	<keywords>IP-телефония</keywords>
	<type>telephony</type>
	<country>ru</country>
</provider>