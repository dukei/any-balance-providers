<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-telephony-dialnetinfo</id>
	<api flags="no_browser"/>
	<name>Dialnet.info</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Dialnet.info</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="phone" name="Телефон дозвона" type="text"/>
		<counter id="user_login" name="Логин" type="text"/>
		<counter id="user_pass" name="Пароль" type="text"/>
		<counter id="minutes" name="Минуты" type="time_interval" format="{@H}:{@min}:{@sec}"/>
		<counter id="traffic" name="Трафик" units=" Мб"/>
	</counters>
	<keywords></keywords>
	<type>telephony</type>
	<country>ru</country>
</provider>