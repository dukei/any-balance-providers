<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-telephony-plusofon</id>
	<api flags="no_browser"/>
	<name>Плюсофон</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Плюсофон https://plusofon.ru/ </p>
	<p>Информация берется из личного кабинета https://portal.plusofon.ru/signin </p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="limit" name="Кредитный лимит" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="available_nums" name="Доступно номеров"/>
		<counter id="on_nums" name="Подключено номеров"/>
		<counter id="agreement" name="Договор" type="text"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="phone" name="Телефон" type="text"/>
		<counter id="all_nums" name="Подключённые номера" type="text"/>
		<counter id="email" name="E-mail" type="text"/>
	</counters>
	<keywords></keywords>
	<type>telephony</type>
	<country>ru</country>
</provider>