<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="4">ab-tracking-qwintry</id>
	<api flags="no_browser"/>
	<name>Qwintry Logistics (Бандеролька)</name>
	<description html="true"><![CDATA[
	<p>Получает информацию о посылке в системе Qwintry Logistics <a href="http://logistics.qwintry.com/">http://logistics.qwintry.com/</a></p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="tracking" name="Номер" type="text"/>
		<counter id="create_time" name="Время" type="time" format="dd/MM/yyyy"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="message" name="Сообщение" type="text"/>
		<counter id="trackingLog" name="История" type="html"/>
	</counters>
	<keywords>Бандеролька Qwintry logistics tracking отслеживание отправлений</keywords>
	<type>tracking</type>
	<country>ru</country>
</provider>