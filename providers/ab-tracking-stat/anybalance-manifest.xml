    <counter id="gp_usru_export_import" name="[ГП] US->RU импорт"/>
    <counter id="gp_gbru_export_import" name="[ГП] GB->RU импорт"/>
    
    <counter id="st_hkru_export_import" name="[ST] HK->RU импорт"/>
    <counter id="st_cnru_export_import" name="[ST] CN->RU импорт"/>
    <counter id="st_usru_export_import" name="[ST] US->RU импорт"/>
    <counter id="st_gbru_export_import" name="[ST] GB->RU импорт"/>
  </counters>
  <keywords>
    Почтовое отправление, Почта России, статистика
  </keywords>
  <type>
    tracking, post
  </type>
</provider>
