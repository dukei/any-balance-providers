<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="8">ab-transport-avtodor</id>
	<api flags="no_browser" />
	<name>Автодор</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе счета в компании Автодор http://avtodor-ts.ru</p>
	<p>Информация берется из личного кабинета http://avtodor-tr.ru/account/index</p>
	]]></description>
	<depends>
		<module id="library"/>
	</depends>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<author>d3vil-st &lt;d3vil.st@gmail.com&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="number" name="Договор" type="text"/>
		<counter id="email" name="Email (логин)" type="text"/>
		<counter id="bonus" name="Бонусные баллы"/>
	</counters>
	<keywords>avtodor</keywords>
	<type>transport</type>
	<country>ru</country>
</provider>