<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
	<EditTextPreference
		title="Login"
		key="login"
		summary="||{@s}"
		obligatory="true"
		dialogTitle="Login"
		positiveButtonText="OK"
		negativeButtonText="Cancel"
		dialogMessage="Enter login to personal account.">
	</EditTextPreference>
	<EditTextPreference
		title="Password"
		key="password"
		summary="||***********"
		obligatory="true"
		dialogTitle="Password"
		positiveButtonText="OK"
		negativeButtonText="Cancel"
		dialogMessage="Enter password to personal account."
		inputType="textPassword">
	</EditTextPreference>
	<ListPreference
		key="cabinet"
		title="Cabinet"
		summary="||{@s}"
		entries="Ebates.ru|Ebates.com"
		entryValues="ru|com"
		defaultValue="com">
	</ListPreference>	
</PreferenceScreen>