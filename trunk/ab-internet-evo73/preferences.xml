<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
	<EditTextPreference
		title="Логин"
		key="login"
		summary="||{@s}"
		obligatory="true"
		dialogTitle="Логин"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите логин, используемый для входа в личный кабинет.">
	</EditTextPreference>
	<EditTextPreference
		title="Пароль"
		key="password"
		summary="||***********"
		obligatory="true"
		dialogTitle="Пароль"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите пароль, используемый для входа в личный кабинет."
		inputType="textPassword">
	</EditTextPreference>
	<ListPreference
		key="type"
		title="Что смотреть"
		summary="||{@s}"
		entries="Автоматически|Интернет|Телефон|Телевидение"
		entryValues="|14|50|111"
		defaultValue="">
	</ListPreference>
</PreferenceScreen>