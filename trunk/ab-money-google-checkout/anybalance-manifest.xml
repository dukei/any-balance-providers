<?xml version="1.0" encoding="utf-8"?>
<provider>
    <id version="13">ab-money-google-checkout</id>
    <name>Google Wallet (Checkout)</name>
	<api flags="no_browser"/>
	<description html="true"><![CDATA[
	<p>Obtains information from Google Wallet personal account. <a href="https://checkout.google.com/sell/orders">https://checkout.google.com/sell/orders</a></p>
	]]></description>
    <author>Stark &lt;support@starksoft.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>login.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
    <counters>
        <counter id="balance" name="Balance" units=" {@currency}"/>
		<counter id="last_payment" name="Last payment" units=" {@currency}"/>
		<counter id="next_payment" name="Next payment" units=" {@currency}"/>
		<counter id="last_payment_date" name="Last payment date" type="time" format="dd/MM/yyyy" />
		<counter id="fio" name="Name" type="text"/>
		<counter id="currency" name="Currency" type="text"/>
		<counter id="login_email" name="E-Mail" type="text"/>
    </counters>
    <keywords>Google, Play, Android, market, Checkout, wallet, walet</keywords>
    <type>money, wallet</type>
</provider>