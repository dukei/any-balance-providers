<?xml version="1.0" encoding="utf-8"?>
<provider>
  <id major_version="1" version="2">ab-money-handelsbanken</id>
  <api flags="no_browser" />
  <name>Handelsbanken</name>
  <description>
	Reads the information from Handelsbanken account (http://m.handelsbanken.se)
  </description>
  <author>valeravi &lt;valeravi@vi-soft.com.ua&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>library.js</js>
    <js>main.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="balance" name="Balance" units=" {@currency}"/>
    <counter id="accnum" name="Account number" type="text"/>
    <counter id="currency" name="Currency" type="text"/>
  </counters>
  <keywords>
    money, bank, handelsbanken
  </keywords>
  <type>
    money
  </type>
  <country>
    se
  </country>
</provider>
