<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="1">ab-money-rosdorbank</id>
	<api flags="no_browser"/>
	<name>Росдорбанк</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе на картах Росдорбанка http://www.rdb.ru</p>
	<p>Информация берется из личного кабинета http://www.rdb.ru/cardvyp.html</p>
	]]></description>
	<author>Stark &lt;support@starksoft.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="available" name="Доступный остаток" units=" {@currency}"/>
		<counter id="limit" name="Кредитный лимит" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="fio" name="ФИО" type="text"/>
		<counter id="cardnumber" name="Номер карты" type="text"/>
		<counter id="status" name="Статус" type="text"/>
		<counter id="avail_till" name="Срок действия" type="time" format="dd/MM/yyyy"/>
	</counters>
	<keywords></keywords>
	<type>money</type>
	<country>ru</country>
</provider>