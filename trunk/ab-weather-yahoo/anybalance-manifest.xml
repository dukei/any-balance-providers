<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="2">ab-weather-yahoo</id>
	<api flags="no_browser"/>
	<name>Yahoo! Weather</name>
	<description html="true"><![CDATA[
	<p>Shows current weather in entered city. Information obtains from <a href="http:/weather.yahoo.com">http://weather.yahoo.com/</a></p>
	]]></description>
	<author>Stark &lt;support@starksoft.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="current_temp" name="小urrent temperature" units="{@degrees_units}"/>
		<counter id="current_pressure" name="小urrent pressure" units="{@pressure_units}"/>
		<counter id="current_humidity" name="小urrent humidity" units="%"/>
		<counter id="current_text" name="小urrent condition" type="text"/>
		<counter id="sunrise" name="Sunrise" type="text"/>
		<counter id="sunset" name="Sunset" type="text"/>
		<counter id="wind" name="Wind speed" units="{@wind_units}"/>
	</counters>
	<keywords></keywords>
	<type>weather</type>
	<country></country>
</provider>